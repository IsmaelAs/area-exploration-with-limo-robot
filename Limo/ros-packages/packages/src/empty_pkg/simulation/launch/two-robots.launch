<?xml version="1.0"?>
<launch>

    <group ns="limo1">
        <param name="tf_prefix" value="limo1" />
        <include file="$(find empty_pkg)/simulation/launch/one-robot.launch"> 
            <arg name="robot_namespace" value="/limo1" />
        </include>
    </group>

    <group ns="limo2">
        <param name="tf_prefix" value="limo2" />
        <include file="$(find empty_pkg)/simulation/launch/one-robot.launch"> 
            <arg name="robot_namespace" value="/limo2" />
            <arg name="x" value="-2"/>
        </include>
    </group>
</launch>