<?xml version="1.0"?>
<launch>
  <arg name="ns" default="/limo1"/>

    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen" ns="$(arg ns)">
      <!--param name="scan" value="/limo1/scan"/-->
      <remap from="scan" to="$(arg ns)/scan"/> 
      <param name="base_frame" value="$(arg ns)/base_footprint"/>
      <param name="odom_frame" value="$(arg ns)/odom"/>
      <param name="map_frame"  value="$(arg ns)/map"/>
    </node>
</launch>
