<launch>
  <param name="/use_sim_time" value="true" />

  <!-- start world -->
  <node name="gazebo1" pkg="gazebo_ros" type="gazebo" 
   args="$(find limo_gazebo_sim)/worlds/empty.world" respawn="false" output="screen" />

  <!-- start gui -->
  <!--node name="gazebo_gui" pkg="gazebo" type="gui" respawn="false" output="screen"/-->

  <!-- include our robots -->
  <include file="$(find limo_gazebo_sim)/launch/two_limo_ackerman.launch"/>
</launch>